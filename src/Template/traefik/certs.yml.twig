tls:
  certificates:
    - certFile: /certs/cert.pem
      keyFile: /certs/key.pem
  stores:
    default:
      defaultCertificate:
        certFile: /certs/cert.pem
        keyFile: /certs/key.pem

http:
  routers:
    traefik-dashboard:
      rule: "Host(`traefik.{{ project_name }}.local`)"
      entryPoints:
        - websecure
      service: api@internal
      tls: {}
