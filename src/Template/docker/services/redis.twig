  {{ name }}:
    image: redis:{{ service.version }}
    container_name: seaman-{{ name }}
    ports:
      - "${REDIS_PORT}:6379"
{% if labels is defined and labels|length > 0 %}
    labels:
{% for label in labels %}
      - "{{ label }}"
{% endfor %}
{% endif %}
    networks:
      - seaman
{% if volumes.shouldPersist(name) %}
    volumes:
      - {{ name }}:/data
{% endif %}
    healthcheck:
      test: ["CMD", "redis-cli", "ping"]
      interval: 10s
      timeout: 5s
      retries: 5
