  {{ name }}:
    image: memcached:{{ service.version }}
    container_name: seaman-{{ name }}
    ports:
      - "${MEMCACHED_PORT}:11211"
{% if labels is defined and labels|length > 0 %}
    labels:
{% for label in labels %}
      - "{{ label }}"
{% endfor %}
{% endif %}
    networks:
      - seaman
