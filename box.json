{
  "output": "build/seaman.phar",
  "directories": [
    "src",
    "helpers"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "vendor-hotfix",
        "Tests",
        "tests",
        "test"
      ],
      "in": "vendor"
    }
  ],
  "files": [
    "composer.json"
  ],
  "compression": "GZ",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php"
  ]
}
